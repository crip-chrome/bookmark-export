{
  "manifest_version": 2,
  "name": "Chrome bookmark export tool",
  "description": "Exports bookmarks to http://hrefs.crip.lv with one click",
  "version": "0.1.1",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "Export bookmarks"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "http://*/"
  ]
}